{
  "NginxLauncher": true,
  "NginxServiceName": "NginxServerFarms",
  "Nginx": {
    "ConfigFileDir": "c:/tools/nginx-1.19.2/conf",
    "ConfigFileName": "nginx.conf",
    "ConfigFileWatchDebounceTimeMs": 250,
    "ProcessFileDir": "c:/tools/nginx-1.19.2",
    "ProcessFileName": "nginx.exe"
  },
  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001",
        "Certificate": {
          "Path": "c:/suite/nginx/server-farms.pfx",
          "Password": "password"
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}